{
  "name": "sirius",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "@cfxjs/react-ui": "2.0.0-alpha.4",
    "@geist-ui/react-icons": "1.0.0",
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "10.0.1",
    "@types/fontfaceobserver": "0.0.6",
    "@types/jest": "25.1.4",
    "@types/node": "13.9.3",
    "@types/react": "16.9.25",
    "@types/react-dom": "16.9.3",
    "@types/react-helmet": "5.0.15",
    "@types/react-router-dom": "5.1.3",
    "@types/react-test-renderer": "16.9.2",
    "@types/styled-components": "5.0.1",
    "@types/testing-library__jest-dom": "5.0.2",
    "@types/webpack-env": "1.15.1",
    "ace-builds": "1.4.12",
    "ace-mode-solidity": "0.1.1",
    "canvas": "2.6.1",
    "clsx": "1.1.1",
    "cross-env": "7.0.2",
    "d3-array": "2.8.0",
    "d3-scale": "3.2.3",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.2",
    "fontfaceobserver": "2.1.0",
    "husky": "4.2.3",
    "i18next": "19.3.4",
    "i18next-browser-languagedetector": "4.0.2",
    "jest-styled-components": "7.0.2",
    "lint-staged": "10.0.8",
    "node-plop": "0.25.0",
    "numeral": "2.0.6",
    "plop": "2.6.0",
    "prettier": "2.0.1",
    "query-string": "6.13.1",
    "react": "16.13.0",
    "react-ace": "9.1.3",
    "react-app-polyfill": "1.0.6",
    "react-dom": "16.13.0",
    "react-helmet-async": "1.0.6",
    "react-i18next": "11.3.4",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.4.3",
    "react-test-renderer": "16.13.0",
    "react-use": "15.3.4",
    "recoil": "0.0.10",
    "sanitize.css": "11.0.0",
    "serve": "11.3.0",
    "shelljs": "0.8.3",
    "styled-components": "5.0.1",
    "stylelint": "13.2.1",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "swr": "0.3.2",
    "ts-node": "8.8.2",
    "typescript": "3.8.2",
    "webfontloader": "1.6.28"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "test:generators": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/testing/test-generators.ts",
    "start:prod": "yarn build && serve -s build",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "yarn eslint -- src",
    "lint:fix": "yarn eslint -- --fix src",
    "lint:css": "stylelint src/**/*.css",
    "generate": "cross-env TS_NODE_PROJECT='./internals/ts-node.tsconfig.json' plop --plopfile internals/generators/plopfile.ts",
    "prettify": "prettier --write"
  },
  "browserslist": {
    "production": [
      ">1%",
      "not IE 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=10.13.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run eslint -- --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run checkTs && lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/loadable.{js,jsx,ts,tsx}",
      "!src/**/*/types.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!src/setupProxy.js"
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ],
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "master",
      {
        "name": "dev",
        "channel": "alpha",
        "prerelease": "alpha"
      }
    ]
  },
  "devDependencies": {
    "@testing-library/user-event": "12.1.6",
    "canvas": "2.6.1",
    "cz-conventional-changelog": "3.3.0",
    "http-proxy-middleware": "1.0.5",
    "jest-environment-jsdom-sixteen": "1.0.3",
    "jest-fetch-mock": "3.0.3",
    "semantic-release": "17.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
